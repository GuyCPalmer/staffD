<div class="container profileInfo z-depth-3">
  <div class="container">
    <div class="media">
      <h5 class="userName">{{user.talentName}}</h5>
      <img class="mr-3 profileImage" src="https://picsum.photos/200" alt="Generic placeholder image">
      <div class="media-body">
        <p class="userInfo">Rating: 10/10</p>
        <p class="userInfo"></p>
        <ul class='userInfo'>
          <li class="job" alt="Bartender" value="{{user.InfoBar}}">Bartender</li>
          <li class="job" alt="Server" value="{{user.InfoServer}}">Server</li>
          <li class="job" alt="Sales" value="{{user.InfoSales}}">Sales</li>
          <li class="job" alt="Promo Model" value="{{user.InfoModel}}">Promo Model</li>
          <li class="job" alt="Security" value="{{user.InfoSecurity}}">Security</li>
        </ul>
        <p class="userInfo">{{user.previousEvents}}</p>
      </div>
    </div>
    <div class="col-s4">
      <p class="userInfo">{{user.bio}}</p>
    </div>
  </div>
</div>
  <script src="../assets/js/eventList.js" type="text/javascript"></script>
