<div class="row">
    <form class="col s12" id="talentSearch">
        <label>Position:</label>
        <div class="row">
            <p class="job">
                <label>
                    <input type="checkbox" class="filled-in" name="Bartender" id="Bartender" />
                    <span>Bartender |</span>
                </label>
            </p>
            <p class="job">
                <label>
                    <input type="checkbox" class="filled-in" name="Server" id="Server" />
                    <span>Server |</span>
                </label>
            </p>
            <p class="job">
                <label>
                    <input type="checkbox" class="filled-in" c name="promoModel" id="promoModel" />
                    <span>Promo Model |</span>
                </label>
            </p>
            <p class="job">
                <label>
                    <input type="checkbox" class="filled-in" name="Security" id="Security" />
                    <span>Security |</span>
                </label>
            </p>
            <p class="job">
                <label>
                    <input type="checkbox" class="filled-in" name="Sales" id="Sales" />
                    <span>Sales |</span>
                </label>
            </p>
            <a class="waves-effect waves-light btn" id="searchBtn" type="submit">
                <i class="material-icons right" id="searchJob">search</i>Search</a>
        </div>
    </form>

    <ul class="talent">

        {{#each talent}} {{>talent-li/talent-util}} {{/each}}

    </ul>

</div>



<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/js/materialize.min.js"></script>

<script src="../assets/js/rating.js" type="text/javascript"></script>